# Generate a random secret: https://generate-secret.vercel.app/32 or `openssl rand -base64 32`
AUTH_SECRET=****

# The following keys below are automatically created and
# added to your environment when you deploy on Vercel

# Instructions to create a Vercel Blob Store here: https://vercel.com/docs/vercel-blob
BLOB_READ_WRITE_TOKEN=****

# Instructions to create a PostgreSQL database here: https://vercel.com/docs/postgres
POSTGRES_URL=****


# Instructions to create a Redis store here:
# https://vercel.com/docs/redis
REDIS_URL=****

# OpenRouter API key for LLM access
# Get your API key from https://openrouter.ai/keys
OPENROUTER_API_KEY=****

# Spotify API credentials
SPOTIFY_CLIENT_ID=your_client_id
SPOTIFY_CLIENT_SECRET=your_client_secret
SPOTIFY_REDIRECT_URI=http://localhost:3000/api/auth/spotify/callback

# Google API credentials (Calendar & Gmail)
# Get your credentials from https://console.cloud.google.com/apis/credentials
# Required scopes: calendar, calendar.events, gmail.readonly, gmail.modify, gmail.labels
GOOGLE_CLIENT_ID=your_client_id
GOOGLE_CLIENT_SECRET=your_client_secret
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/google/callback

# Suppress Turbopack warning
SERWIST_SUPPRESS_TURBOPACK_WARNING=1

# Vercel Blob token
BLOB_READ_WRITE_TOKEN=your_blob_read_write_token

# Enable registration
NEXT_PUBLIC_REGISTRATION_ENABLED=true

# Qdrant Vector Database (ON HOLD - OpenRouter doesn't support embeddings)
# The semantic chat history search feature requires embedding models which
# OpenRouter does not currently support. These variables are kept for future use.
# Local: http://localhost:6333 (using docker-compose)
# Cloud: Get from https://cloud.qdrant.io
QDRANT_URL=
QDRANT_API_KEY=